{
  "name": "broxus-docs-kit-dev",
  "version": "1.0.7",
  "description": "Broxus Docs Kit is a Vue.js and VitePress-based toolkit for streamlining Broxus project documentation. It offers ready-to-use templates and custom components, ensuring consistent, up-to-date, and clear documentation built on a user-friendly tech stack.",
  "license": "MIT",
  "repository": "https://github.com/cyace84/broxus-docs-kit",
  "author": "Valentyn Zavizion",
  "main": "dist/index.js",
  "bin": {
    "broxus-docs-kit-dev": "bin/broxus-docs-kit"
  },
  "exports": {
    ".": "./dist/index.js",
    "./styles": "./dist/styles/"
  },
  "files": [
    "/bin",
    "/dist",
    "/src",
    "/template",
    "/LICENSE",
    "/package.json"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:tsc": "tsc",
    "build:components": "node ./scripts/build-components.js",
    "build": "npm run lint && npm run build:clean && npm run build:tsc && npm run build:components",
    "lint": "eslint 'src/**/*.{js,ts,vue}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,vue}' --fix"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "everscale-inpage-provider": "0.3.61",
    "everscale-standalone-client": "2.1.19",
    "fs-extra": "^11.1.1",
    "prompts": "^2.4.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.9",
    "vitepress": "1.0.0-beta.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@types/escodegen": "^0.0.7",
    "@types/esprima": "^4.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "escodegen": "^2.0.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-vue": "^9.15.1",
    "rollup": "^3.25.1",
    "sass": "^1.63.6",
    "typescript": "5.0.4"
  }
}
